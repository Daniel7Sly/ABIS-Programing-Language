@main:
    arg     2
    arg     7
    
    exe     @Power
    test    @

    rtn _

@Square:
    var numb x
    giv $x @

    mul $x $x

    arg @
    fre $x
    rtn @

@Power:
    var numb base giv $base @
    var numb expo giv $expo @
    smt $expo 1
    ift @ PowerEnd

    var numb c giv $c 1

    arg 1
    Loop:
        mul @ $base
        LoopEnd:
            add $c 1 giv $c @
            smt $c $expo
            eql $c $expo
            orr @ @
            ift @ Loop
    
    PowerEnd:
        fre $base fre $expo fre $c
    rtn @



